<h1>ログイン</h1>
<%= form_with url: login_path, local: true do |f| %>
<div class="field">
    <%= f.label :email %>
    <%= f.text_field :email %>
</div>
<div class="field">
    <%= f.label :password %>
    <%= f.password_field :password %>
</div>
<div>
    <%= f.submit "ログイン" %>
</div>
<%= link_to "Sign up", new_user_path %>
<% end %>

